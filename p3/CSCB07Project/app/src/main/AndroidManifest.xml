<?xml version="1.0" encoding="utf-8"?>
<manifest package="csc.project"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".UserControl"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <service
            android:name=".SaveService"
            android:exported="false"/>

        <activity
            android:name=".login.LoginActivity"
            android:label="@string/title_activity_login">
        </activity>
        <activity
            android:name=".main.MainActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name=".login.StartActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:finishOnTaskLaunch="true"
            android:label="@string/app_name"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".login.RegisterActivity"
            android:label="@string/title_activity_register"
            android:noHistory="true">
        </activity>
        <activity android:name=".login.ForgotActivity">
        </activity>
        <activity android:name=".interact.EditTravelActivity">
        </activity>
        <activity android:name=".interact.ViewItineraryActivity">
        </activity>
        <activity android:name=".interact.EditUserActivity">
        </activity>
        <activity android:name=".interact.SearchItineraryActivity">
        </activity>
        <activity android:name=".interact.SearchTravelActivity">
        </activity>
    </application>

</manifest>
